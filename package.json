{
  "name": "dev-enviro",
  "version": "1.0.0",
  "description": "A dev environment with setup for Git, NodeJS, TypeScript, etc.",
  "scripts": {
    "start": "npm run open:src",
    "open:src": "ts-node buildScripts/srcServer.ts",
    "startOLD_JS": "npm run openOLDJS:src",
    "openOLDJS:src": "babel-node buildScripts/srcServer.js",
    "startOLD": "npm-run-all --parallel security-check open:src tslint:watch test-typescript:watch",
    "startWithoutSecurity": "npm-run-all --parallel open:src tslint:watch test-typescript:watch",
    "startWithoutTests": "npm-run-all --parallel security-check open:src tslint:watch",
    "tslint": "tslint src/**/*.ts src/**/*.tsx",
    "tslint:watch": "onchange \"src/**/*.ts\" \"src/**/*.tsx\" -- npm run tslint",
    "security-check": "nsp check",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist tslint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "copy-to-common-resources": "babel-node buildScripts/copyDistToCommonResources.js",
    "test-typescript": "mocha -c -r ts-node/register --reporter list \"src/**/*.test.tsx\"",
    "posttest-typescript": "babel-node buildScripts/startTestingMessage.js",
    "test-typescript:watch": "watch-run -i -p \"src/**/*.tsx\",\"src/**/*.ts\" npm run test-typescript",
    "OLD SCRIPTS:": "Scripts below here are old or unused...",
    "share": "npm-run-all --parallel open:src localtunnel",
    "localtunnel": "lt --port 3000",
    "exposeOnLocalTunnel": "npm-run-all --parallel localtunnel start",
    "lint": "esw webpack.config.*  src  buildScripts",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test_ORIGINAL": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "start_OLD": "npm-run-all --parallel security-check open:src tslint:watch test:watch start-mockapi-staff",
    "generate-mock-data": "babel-node buildScripts/JSONMockData/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "prestart-mockapi-staff": "npm run generate-mock-data",
    "start-mockapi-staff": "json-server --watch src/components/staffDirectorySearch/api/db.json --port 3001",
    "testTypeScript:watch_OLD": "npm run testTypeScript -- --watch"
  },
  "author": "Michael Mundy",
  "license": "MIT",
  "dependencies": {
    "event-source-polyfill": "0.0.12",
    "path": "^0.12.7",
    "react": "^16.2.0"
  },
  "devDependencies": {
    "@types/webpack": "^3.8.7",
    "awesome-typescript-loader": "^3.4.1",
    "body-parser": "^1.18.2",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "open": "0.0.5",
    "react-hot-loader": "^3.1.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-middleware": "^2.0.5",
    "webpack-hot-middleware": "^2.21.0"
  }
}
